{
    "variable": [
        {
            "name": [
                {
                    "type": "string",
                    "default": "env3",
                    "description": "Nombre del servidor local"
                }
            ]
        },
        {
            "network": [
                {
                    "type": "string",
                    "default": "net2-peered",
                    "description": "Red de despliegue del servidor"
                }
            ]
        },
        {
            "port": [
                {
                    "type": "number",
                    "default": 3000,
                    "description": "Puerto de la aplicacion"
                }
            ]
        },
        {
            "api_key": [
                {
                    "type": "string",
                    "default": "default-secret-key",
                    "description": "API key para autenticacion",
                    "sensitive": true
                }
            ]
        }
    ]
}